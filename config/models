User
    ident Text
    password Text Maybe
    role Role
    familyName Text
    givenName Text
    email Text
    avatar FileHeaderId Maybe
    active Bool default=true
    UniqueUser ident
    
Profile
    user UserId
    birth Day
    
    entryYear Int
    graduateYear Int Maybe
    branch Text
    
    zip Text
    address Text
    longitude Double Maybe
    latitude Double Maybe
    tel Text
    station Text
    
    homeZip Text
    homeAddress Text
    homeLongitude Double Maybe
    homeLatitude Double Maybe
    homeTel Text
    
    desiredCourse Text Maybe
    desiredWorkLocation Text Maybe
    employment Text Maybe
    
    UniqueProfile user

Laboratory
    headResearcher UserId
    roomNumber Text Maybe
    extensionNumber Text Maybe
    courses Text Maybe
    UniqueLaboratory headResearcher

Email
    email Text
    user UserId Maybe Update
    verkey Text Maybe Update
    UniqueEmail email

Project
    name Text
    description Text
    statuses Text
    issuecounter IssueNo default=0
    terminated Bool default=false
    cuser UserId
    cdate UTCTime default=now()
    udate UTCTime default=now()

Issue
    project ProjectId
    number IssueNo
    subject Text
    assign UserId Maybe
    status Text
    limitdate Day Maybe
    cuser UserId
    cdate UTCTime default=now()
    uuser UserId
    udate UTCTime default=now()
    UniqueIssue project number

Comment
    project ProjectId
    issue IssueId
    content Text Maybe
    automemo Text
    assign UserId Maybe
    status Text
    limitdate Day Maybe
    attached FileHeaderId Maybe
    checkReader Bool default=false
    cuser UserId
    cdate UTCTime default=now()

Reader
    comment CommentId
    reader UserId
    checkdate UTCTime default=now()
    UniqueReader comment reader

Participants
    project ProjectId
    user UserId
    receivemail Bool default=true
    cdate UTCTime default=now()
    UniqueParticipants project user

FileHeader
    fullname Text
    efname Text
    name Text
    extension Text
    contentType Text
    fileSize Int64
    creator UserId
    created UTCTime default=now()

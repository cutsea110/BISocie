@charset "utf-8";
body{
	font-family:sans-serif;
	font-size: 14px;
	margin:0;
	padding:0;
	width: 100%;
	background: #f7f6f2;
}

body, html
{
    height: 100%;
}

h1, h2, h3, h4, h5, h6{
	margin:0;
	padding:0;
}

ul, ol{	-webkit-padding-start:15px;}
ul{	list-style-type:square;}
img{	border-style:none;}
pre{
	padding:3px;
	border:1px solid #333333;
	white-space:pre-wrap;
	word-wrap:break-all;
}
.help{
	font-size:70%;
	color:#888;
}

input { width: 600px; }
input[type="radio"], input[type="checkbox"] { width: 20px; }

/*** TABLED ***/
table {
	border-collapse: collapse;
}

table a {	color: #9f7b7f;}

table.list{
	font-size:86%;
	width:90%;
	margin: 0 auto; 
	border-collapse:collapse;
	background-color:#ffffff;
	color: #656460;
}

table.list th.assign{	width:9%}
table.list th.date{	width:15%}
table.list th.desc{	width:30%}
table.list th.ino{ width:3%}
table.list th.limit{ width:9%}
table.list th.name{ width:30%}
table.list th.pname{ width:15%}
table.list th.sort-header{
	text-decoration:none;
	color:#4040ff
}
table.list th.status{	width:8%}
table.list th.subject{ width:20%}
table.list th.uname{ width:10%}

tr.more{ background-color:#cccccc}

tr.more div{
	font-size:110%;
	font-weight:bold;
	color:#3040f0;
	margin:5px;
	text-align:center
}

tr.more div:hover{ cursor:pointer}

table.nostyle, table.nostyle tr, table.nostyle th, table.nostyle td{
	border:none;
	background-color:white;
}

table.list tr:nth-child(odd){ background-color:#ffffff;}
table.list trnth-child(even){	background-color:#dddddd;}
table.list td{ border:solid 1px #656460;}
table.list tr, table.list th{
	border:solid 1px #656460;
	background-color:#f0dfe5;
}
table.list thead td.left, table.list tfoot td.left{border-right:none;}
table.list thead td.center, table.list tfoot td.center{
	border-left:none;
	border-right:none;
	text-align:left;
}
table.list thead td.right, table.list tfoot td.right{border-left:none;}

td.number{text-align:right;}

.Strike td{	text-decoration:line-through}

.searchbox{
	width: 90%;
	margin: 0 auto;
	padding:10px;
	margin-bottom:5px;
	border:solid 1px #CCC;
	background-color:ivory;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	-o-border-radius: 4px;
	-ms-border-radius: 4px;
	border-radius: 4px;
}

.searchbox caption {
/***	margin-right: 10px;***/
	margin: 10px;
	font-size: 12px;
	font-weight: bold;
	color: #895d5c;
}

.searchbox label, .searchbox input {
	margin-right: 10px;
	width: 200px;
}

.searchbox input[type="checkbox"] {
	width: 20px;
}

.searchbox table th { padding: 4px; }
.searchbox table td { padding-left: 10px; }

.paging-link{
	margin-left:0.2em;
	margin-right:0.2em;
}
div#message{
	color:#817134;
	backgroud-color:#fdf1d0;
	border:1px solid #fad163;
	line-height:1.50;
	padding:5px 8px;
	margin:0 0 3px 0
}
.errors, .alert{
	color:red;
	font-style:bold;
}
.warnings{
	color:red;
	font-style:bold;
	backgroud-color:#fdf1d0;
	border:1px solid #fad163;
	line-height:1.50;
	padding:5px 8px;
	margin:0 0 3px 0
}
span.invalid{
	color:red;
	font-style:bold;
}
input.invalid, textarea.invalid{
	border:1px solid red;
}


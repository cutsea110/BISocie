	border-bottom:solid 1px #cccccc;
	color:#333333;
	margin-top:1em
}

dd{
	border:solid 1px #cccccc;
	padding:1em;
	margin-left:0em;
	margin-top:0.5em
}

img.mail.clickable:hover{
	cursor:pointer
}

.errors,.alert{
	color:red;
	font-style:bold
}

input.invalid,textarea.invalid{
	border:1px solid red
}

/*** PROFILE ***/

table.profile {
		margin: 0 auto;
    border: 1px solid #B7B9C5;
    border-collapse: collapse;
    width: 700px;
}
table.profile th {
    background-color: #F7E5E5;
    border: 1px solid #B7B9C5;
    text-align: left;
    padding: 8px;
}
table.profile td {
    border: 1px solid #B7B9C5;
    text-align: left;
    padding: 8px;
    background: #fffffd;
}
th.number, td.number {
    text-align: right;
}
#map_canvas {
    height: 200px;
    margin-top: 5px;
    width: 400px;
}
#area_map {
    height: 400px;
    margin-top: 5px;
    width: 460px;
}
#myavatar {
    width: 100px;
}
#avatar {
    text-align: center;
}
input[type="address"] {
    width: 400px;
}
input[type="email"] {
    width: 200px;
}
input[type="tel"] {
    width: 160px;
}
input[name="familyName"], input[name="givenName"] {
    width: 100px;
}
input[type="zip"] {
    width: 80px;
}
input[name="roomnumber"] {
    width: 80px;
}
input[name="extensionnumber"] {
    width: 80px;
}
textarea[name="courses"] {
    height: 10em;
    width: 320px;
}

